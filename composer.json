{
    "name": "bobdenotter/yaml-migrate",
    "description": "A library to facilitate migrations for YAML configuration files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "require": {
        "nategood/commando": "^0.4.0",
        "webimpress/safe-writer": "^2.1",
        "composer/semver": "^3.2",
        "symfony/yaml": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/finder": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "YamlMigrate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YamlMigrate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^8.3",
        "phpunit/phpunit": "^8.5"
    }
}
